  "grammars": [
    {
      "name": "toon",
      "camelcase": "Toon",
      "title": "Toon",
      "scope": "source.toon",
      "file-types": ["toon"],
      "injection-regex": "^toon$",
      "class-name": "TreeSitterToon"
    }
  ],
  "metadata": {
    "version": "0.1.3",
    "license": "MIT",
    "description": "Toon grammar for tree-sitter",
    "authors": [
      {
        "name": "3swordman",
        "email": "yyuxiaoran@qq.com",
        "url": "https://github.com/3swordman"
      }
    ],
    "links": {
      "repository": "https://github.com/3swordman/tree-sitter-toon"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
